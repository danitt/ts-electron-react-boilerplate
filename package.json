{
  "name": "ts-electron-react-boilerplate",
  "title": "TypeScript Electron React Boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "Danitt <dan@danitt.com>",
  "license": "UNLICENSED",
  "main": "main.js",
  "scripts": {
    "start": "yarn serve",
    "serve": "webpack-dev-server --env.START_HOT --config webpack/webpack.renderer.js",
    "serve:main": "yarn build:main && electron ./dist/main.js",
    "build": "yarn build:main && yarn build:renderer",
    "build:production": "yarn build:main --env.PRODUCTION && yarn build:renderer --env.PRODUCTION",
    "build:main": "webpack --config webpack/webpack.main.js",
    "build:renderer": "webpack --config webpack/webpack.renderer.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:tsc": "npx tsc --noEmit",
    "test": "jest",
    "package": "yarn build:production && electron-packager ./dist \"AppName\" --arch=x64 --platform=darwin --out=bin --icon=./dist/AppIcon --overwrite"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "electron": "^3.1.9",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^14.2.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module-scoped": "^1.0.3",
    "jest": "^25.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.12.0",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "^0.0.4",
    "raw-loader": "^4.0.1",
    "react-hot-loader": "^4.8.8",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  }
}
